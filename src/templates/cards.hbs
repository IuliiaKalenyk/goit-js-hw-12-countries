{{#each this}}
<div class="card__content">
            <h1 class="card__name">{{this.name}}</h2>
            <p class="card__capital">Capital:
                {{capital}}
            </p>

            <p class="card__population">Population:
                {{population}}
            </p>
            <p class="card__languages">Languages:</p>
            <ul class="tag-list"></ul>
                {{#each languages}}
                <li class="tag-list__item">{{name}}</li>
                {{/each}}
            </ul>
            <img src="{{flag}}" alt="{{name}}" class="card__image" />
</div>
{{/each}}